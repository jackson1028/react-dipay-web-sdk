  "description": "Dipay library built in React",
  "author": "jackson1028",
  "license": "MIT",
  "repository": "https://github.com/jackson1028/react-dipay-web-sdk",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
